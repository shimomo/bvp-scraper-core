{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "bvp/scraper-core",
    "description": "The BVP Scraper Core package for Boatrace.",
    "keywords": [
        "boatrace",
        "library",
        "scraper"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "shimomo",
            "email": "bvp@shimomo.net"
        }
    ],
    "support": {
        "issues": "https://github.com/shimomo/bvp-scraper-core/issues",
        "source": "https://github.com/shimomo/bvp-scraper-core"
    },
    "require": {
        "php": "^8.2",
        "bvp/converter": "^4.0",
        "bvp/trimmer": "^4.0",
        "symfony/browser-kit": "^6.0 || ^7.0",
        "symfony/css-selector": "^6.0 || ^7.0",
        "symfony/http-client": "^6.0 || ^7.0",
        "symfony/mime": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.1 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "BVP\\ScraperCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BVP\\ScraperCore\\Tests\\": "tests/"
        }
    }
}
